TestProject {
    <TestProject> {
        foo(a:<Int32Type>, b:<Int32Type>, s:<<Int8Type>PointerType>) -> <IRFunctionType -> <VoidType>, {}> {
            :begin {
                constant $;
                declare $ $nullptr;
                constant $;
                declare $ $nullptr;
                constant $;
                declare $ $nullptr;
                constant $;
            }
            :fn.foo {
                constant $;
                declare $ $constant;
                constant $;
                declare $ $constant;
                load $declare.VAR;
                load $declare.VAR1;
                lgc_not_equal $load.VAR $load.VAR1;
                load $declare.a;constant $;
                lgc_equal $load.a $constant;
                lgc_and $lgc-not-equal $lgc-equal;
                cond_br $lgc-and $ $;
            }
            :if.then {
                constant $;
                ret $constant;
                br $;
            }
            :if.else {
                br $;
            }
            :if.merge {
                load $declare.VAR1;
                load $declare.s;
                assign $load.VAR1 $load.s;
                load $declare.a;
                load $declare.b;
                add $load.a $load.b;
                ret $add;
            }
        }
    }
}